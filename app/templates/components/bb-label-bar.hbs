{{#each labels as |label|}}
  {{#if template.blockParams}}
      {{yield label}}
    {{else}}
      {{#if label.background}}
        <rect x={{label.background.x}}
              y={{label.background.y}}
              width={{label.background.width}}
              height={{label.background.height}}
        rx="2"/>
      {{/if}}
      <text x=10 y={{label.y}} class="{{label.evalClass}} {{label.labelClass}}">{{label.text}}</text>
  {{/if}}
{{/each}}
